          },
          "MktID": {
            "type": "string",
            "fixmlTypeName": "MarginAmountMarketID_t",
            "description": "Market associated with the margin amount"
          },
          "Dirctn": {
            "type": "string",
            "fixmlTypeName": "MarginDirection_t"
          }
        },
        "fixmlComponent": "MgnAmt"
      },
      "fixmlRepeating": true
    },
